<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company>Nikolay Vorobev</Company>
    <Authors>Nikolay Vorobev</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A library for fast read data with CSV format according to RFC4180 with small extensions. Best solution for parse very large data files</Description>
    <PackageLicenseUrl>https://github.com/bopohaa/CsvParser/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/bopohaa/CsvParser</PackageProjectUrl>
    <RepositoryUrl>https://github.com/bopohaa/CsvParser.git</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>CSV, parser, fast</PackageTags>
    <SignAssembly>false</SignAssembly>
    <AssemblyOriginatorKeyFile>CsvParser.pfx</AssemblyOriginatorKeyFile>
    <Copyright>Copyright Â© 2018 Nikolay Vorobev</Copyright>
    <PackageId>FastCsvParser</PackageId>
    <Version>1.1.0</Version>
    <PackageIconUrl>https://raw.githubusercontent.com/bopohaa/CsvParser/master/icon.png</PackageIconUrl>
  </PropertyGroup>

</Project>
